{
  "name": "focusing-manager",
  "version": "1.38.0",
  "description": "Gravitate-Health Focusing Manager",
  "main": "build/index.js",
  "scripts": {
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --forceExit",
    "test:unit": "jest test/unit.test.ts",
    "test:integration": "jest test/focus.integration.test.ts --forceExit",
    "test:debug": "jest --detectOpenHandles --runInBand",
    "lint": "tsc --noEmit",
    "dev": "ts-node-dev ./src/index.ts",
    "build": "if [ -d \"$PWD/build\" ]; then rm -r \"$PWD/build\"; fi && tsc --incremental && cp -r ./src/templates ./build",
    "start": "node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gravitate-Health/focusing-manager.git"
  },
  "author": "Guillermo Mej√≠as",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gravitate-Health/focusing-manager/issues"
  },
  "homepage": "https://github.com/Gravitate-Health/focusing-manager#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.11",
    "@types/jsdom": "^21.1.7",
    "@types/n3": "^1.26.1",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "nock": "^13.5.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@gravitate-health/lens-execution-environment": "^1.2.0",
    "@kubernetes/client-node": "^1.4.0",
    "axios": "^1.3.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "jsdom": "^22.1.0",
    "liquidjs": "^10.7.1",
    "n3": "^2.0.1",
    "redis": "^4.7.1"
  }
}
